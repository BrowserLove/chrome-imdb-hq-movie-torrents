  "name": "IMDb Torrents",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "Adds Torrent Download Links to IMDb pages (720p, 1080p, etc.).",
  "content_scripts": [ {
    "js": ["axios.min.js", "jquery.slim.min.js", "app.js"],
    "css": ["styles.css"],
    "matches": ["*://www.imdb.com/title/*", "*://yts.ag/*"]
  }]
}
