{
  "name": "IMDb Enhanced",
  "version": "2.2",
  "manifest_version": 2,
  "description": "Adds movie HQ torrent & streaming links to IMDb movie pages.",
  "content_scripts": [ {
    "js": ["vendors/axios.min.js", "vendors/jquery.slim.min.js", "vendors/jquery.number.min.js", "details.js", "widget.js"],
    "css": ["styles.css"],
    "matches": ["*://www.imdb.com/", "*://www.imdb.com/?*", "*://www.imdb.com/title/*", "*://yts.ag/*"]
  }]
}
