{
  "name": "IMDb Enhanced",
  "version": "2.3",
  "manifest_version": 2,
  "description": "Adds movie HQ torrent & streaming links to IMDb movie pages.",
  "content_scripts": [
    {
      "js": ["main.js", "widget.js"],
      "css": ["styles.css"],
      "matches": ["*://www.imdb.com/", "*://www.imdb.com/?*", "*://www.imdb.com/title/*", "*://yts.ag/*"]
    }
  ]
}
